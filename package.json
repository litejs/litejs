{
  "name": "litejs",
  "version": "23.3.0",
  "description": "Full-stack web framework in a tiny package",
  "license": "MIT",
  "author": "Lauri Rooden <lauri@rooden.ee>",
  "keywords": [
    "litejs",
    "framework",
    "http",
    "server",
    "content negotiation",
    "accept",
    "multipart",
    "uploads",
    "form-data"
  ],
  "homepage": "https://litejs.com",
  "repository": "github:litejs/litejs",
  "bugs": "https://github.com/litejs/litejs/issues",
  "main": "index.js",
  "files": [
    "/*.js"
  ],
  "scripts": {
    "test": "TZ='Europe/Tallinn' c8 -r lcov -r text-summary --check-coverage --branches=70 --functions=70 --lines=70 --statements=70 lj test test/index.js --no-color --brief && jshint *.js"
  },
  "devDependencies": {
    "@litejs/cli": "23.3.3"
  },
  "litejs": {
    "build": false,
    "global": "jshint c8",
    "lint": false
  },
  "jshintConfig": {
    "asi": true,
    "esversion": 5,
    "evil": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxdepth": 6,
    "node": true,
    "nonbsp": true,
    "nonstandard": true,
    "quotmark": "double",
    "undef": true,
    "unused": true
  }
}
